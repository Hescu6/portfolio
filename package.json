{
  "name": "portfolio",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "electron": "ng build --base-href ./ && electron .",
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "deploy": "ng build --prod --output-path docs --base-href /portfolio/ && ng --dir=docs && git add . && git commit -m '.' && git push origin master",
    "push": "git add . && git commit -m '.' && git push origin master",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ghpages": "ng build --prod --output-path docs --base-href /portfolio/ ngh --dir=docs"
  },
  "private": true,
